{
  "format": "custom",
  "id": "post",
  "label": "blog_post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Categoria",
          "placeholder": "Categoria do Post",
          "options": ["investimentos", "planejamento", "economia"]
        }
      },
      "titulo": {
        "type": "StructuredText",
        "config": {
          "label": "Título",
          "placeholder": "Texto do post",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "data_da_publicacao": {
        "type": "Timestamp",
        "config": {
          "label": "Data da publicação",
          "placeholder": "Data da publicação"
        }
      },
      "descricao": {
        "type": "StructuredText",
        "config": {
          "label": "Descrição",
          "placeholder": "Descrição do post",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text_block": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
